import axios from 'axios';

// Get Request
axios.get('/userSurvey')
  .then((response) => {
    console.log(response);
  })
  .catch((error) => {
    console.log(error);
  });

axios.post('/userSurvey', {
  price: '$2300',
  area: 'SOHO',
  pet: false
})
.then( (response) => {
  console.log(response);
})
.catch( (error) => {
  console.log(error);
});
